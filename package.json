{
  "name": "@zeropaper/overqwil",
  "version": "0.0.1",
  "description": "An example of an over killed customization of Mantine",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write '**/*.{js,json,md,yml}'",
    "test": "jest --passWithNoTests",
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:sb": "start-storybook -p 6006",
    "docs": "run-p docs:*",
    "docs:vite": "vite build --outDir docs",
    "docs:sb": "build-storybook -o docs/storybook",
    "build": "tsc --skipLibCheck --jsx react -d --sourcemap index.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeropaper/overqwil.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeropaper/overqwil/issues"
  },
  "homepage": "https://github.com/zeropaper/overqwil#readme",
  "lint-staged": {
    "*.tsx?": [
      "eslint --cache",
      "prettier -w"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.0",
    "@mantine/core": "^5.1.6",
    "@mantine/hooks": "^5.1.6",
    "react": ">=16.8.0 <18",
    "react-dom": ">=16.8.0 <18"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@emotion/react": "^11.10.0",
    "@mantine/core": "^5.1.6",
    "@mantine/form": "5.2.0",
    "@mantine/hooks": "^5.1.6",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-vite": "0.2.2",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/dom": "8.16.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@types/eslint": "8.4.5",
    "@types/jest": "28.1.6",
    "@types/react": "17.0.48",
    "@types/react-dom": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "@vitejs/plugin-react": "2.0.1",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.7.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "lint-staged": "13.0.3",
    "micromatch": "4.0.5",
    "npm-run-all": "4.1.5",
    "react": ">=16.8.0 <18",
    "react-dom": ">=16.8.0 <18",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "vite": "3.0.7"
  },
  "dependencies": {
    "@fontsource/titillium-web": "4.5.9"
  }
}
