{
  "name": "@zeropaper/overqwil",
  "version": "0.2.1",
  "description": "An example of an over killed customization of Mantine",
  "engines": {
    "npm": ">=7"
  },
  "main": "index.js",
  "scripts": {
    "clean": "git clean -xfd src && rm -f index.js* index.d.ts",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write '**/*.{js,json,md,yml}'",
    "test": "jest --passWithNoTests",
    "dev": "vite",
    "predocs": "npm run clean",
    "docs": "vite build --outDir docs",
    "build": "tsc --skipLibCheck --jsx react -d --sourcemap index.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeropaper/overqwil.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeropaper/overqwil/issues"
  },
  "homepage": "https://github.com/zeropaper/overqwil#readme",
  "lint-staged": {
    "*.tsx?": [
      "eslint --cache",
      "prettier -w"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.4.1",
    "@mantine/form": "5.4.1",
    "@mantine/hooks": "^5.4.1",
    "@mantine/prism": "^5.4.1",
    "@tabler/icons": "^1.101.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.4.1",
    "@mantine/form": "5.4.1",
    "@mantine/hooks": "^5.4.1",
    "@mantine/prism": "^5.4.1",
    "@tabler/icons": "^1.101.0",
    "@testing-library/dom": "8.18.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/eslint": "8.4.6",
    "@types/jest": "29.0.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-loader": "8.2.5",
    "eslint": "8.24.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "lint-staged": "13.0.3",
    "micromatch": "4.0.5",
    "npm-run-all": "4.1.5",
    "prettier": "^2.7.1",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "6.4.1",
    "ts-jest": "29.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "vite": "3.1.3"
  },
  "dependencies": {
    "@fontsource/titillium-web": "4.5.9"
  }
}